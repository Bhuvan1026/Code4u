from math import *
from collections import *
from sys import *
from os import *

def missingAndRepeating(arr, n):
    # Write your code here
    a=[0,0]
    for i in range(1,len(arr)+1):
        if arr.count(i)==0:
            a[0]=i
        if arr.count(i)==2:
            a[1]=i
       
    return a
