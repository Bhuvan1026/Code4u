class Solution:
    # your task is to complete this function
    # function sort the stack such that top element is max
    # funciton should return nothing
    # s is a stack
    def Sorted(self, s):
        # Code here
        a=[]
        while s:
            a.append(s.pop())
        a.sort()
        for i in a:
            s.append(i)
        return s
            
